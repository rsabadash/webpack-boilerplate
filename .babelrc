{
	"presets": [
		["@babel/preset-env",
			{
				"useBuiltIns": "usage",
				"corejs": {
					"version": 3,
					"proposals": true
				},
				"modules": false
			}
		],
		 "@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-object-rest-spread",
		"@babel/plugin-proposal-class-properties",
		["@babel/plugin-transform-runtime", {
			"corejs": {
				"version": 3,
				"proposals": true
			},
			"useESModules": true
		}]
	]
}
